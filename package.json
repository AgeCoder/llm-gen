{
  "name": "llm-gen",
  "version": "1.0.1",
  "description": "A CLI tool to extract text from a static Next.js export and generate llm.txt for LLM ingestion.",
  "main": "index.js",
  "bin": {
    "llm-gen": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build": "node index.js ./out",
    "dev": "node --watch index.js ./out",
    "lint": "eslint . --ext .js,.mjs",
    "format": "prettier --write ."
  },
